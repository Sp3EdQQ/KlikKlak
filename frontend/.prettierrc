{
  "printWidth": 90,
  "tabWidth": 2,
  "useTabs": false,
  "semi": false,
  "singleQuote": false,
  "jsxSingleQuote": false,
  "trailingComma": "none",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "avoid",
  "htmlWhitespaceSensitivity": "strict",
  "quoteProps": "preserve",
  "tailwindFunctions": ["clsx", "twMerge"],
  "plugins": ["prettier-plugin-tailwindcss"],
  "endOfLine": "lf"
}
